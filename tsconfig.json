{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "src",
    "paths": {
      "~/*": [
        "src/*"
      ]
    },
    "verbatimModuleSyntax": true,
    "ignoreDeprecations": "5.0",
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}