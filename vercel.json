{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/mainnet/:folder/:slug.md",
      "destination": "/mainnet/:folder",  // Mengarahkan ke index.astro di direktori induk
      "statusCode": 302
    },
    {
      "source": "/testnet/:folder/:slug.md",
      "destination": "/testnet/:folder",  // Mengarahkan ke index.astro di direktori induk
      "statusCode": 302
    }
  ]
}
