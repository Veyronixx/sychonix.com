{
  "rewrites": [
    {
      "source": "/:path*/",
      "destination": "/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/mainnet/:dir*/:file*.md",
      "destination": "/mainnet/:dir*/",
      "permanent": false
    },
    {
      "source": "/testnet/:dir*/:file*.md",
      "destination": "/testnet/:dir*/",
      "permanent": false
    }
  ]
}
