{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/mainnet/:slug",
      "destination": "/mainnet",
      "has": [
        {
          "type": "query",
          "key": "slug"
        }
      ],
      "statusCode": 302
    },
    {
      "source": "/testnet/:slug",
      "destination": "/testnet",
      "has": [
        {
          "type": "query",
          "key": "slug"
        }
      ],
      "statusCode": 302
    }
  ]
}
